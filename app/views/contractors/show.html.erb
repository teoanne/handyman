<div class="container">
<p id="notice"><%= notice %></p>
<%= image_tag @contractor.image.url(:medium) %>

<p>
  <strong>Name:</strong>
  <%= @contractor.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @contractor.address %>
</p>
<% if @contractor_user == current_user %>
<%= link_to 'Edit', edit_contractor_path(@contractor) %> |
<% end %>
<%= link_to 'Back', contractors_path %>
</div>
